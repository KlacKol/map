              "properties": {
                "lat": { "type": "number" },
                "lng": { "type": "number" },
                "description": { "type": "string" },
                "date": { "type": "Date" }
              }
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Successfully update"
          }
        }

    "/map/search": {
      "post": {
        "tags": [
          "Map"
        ],
        "description": "Create new coordinates",
        "parameters": [
          {
            "name": "map",
            "in": "body",
            "description": "User that we want to create",
            "schema": {
              "properties": {
                "lat": { "type": "number" },
                "lng": { "type": "number" },
                "description": { "type": "string" },
                "date": { "type": "Date" }
              }
            }
          }
        ],
        "produces": [
          "application/json"

    "/map/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "description": "ID of map that we want to find",
          "type": "string"
        }
      ],
      "get": {
        "summary": "get coordinates with id",
        "tags": [
          "Map"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Geo"
            }
          }
        }
      },
      "delete": {
        "summary": "delete coordinates with id",
        "tags": [
          "Map"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Geo"
            }
          }
        }
      },
      "put": {
        "summary": "update coordinates with id",
        "tags": [
          "Map"
        ],
        "description": "Update coordinates",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "type": "string",
            "description": "Update",
            "schema": {
              "properties": {
                "id": {"type": "string"},
                "uniqueItems": true
              }
            }
          },
